{
  "name": "linkedin-ad-redirect-tester",
  "version": "1.0.0",
  "description": "Automated testing tool for LinkedIn ad redirections across multiple devices",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "PLAYWRIGHT_BROWSERS_PATH=./playwright-browsers node test-my-ad.js",
    "test:url": "PLAYWRIGHT_BROWSERS_PATH=./playwright-browsers node src/cli.js",
    "test:real-devices": "node src/aws-device-farm-cli.js",
    "list-real-devices": "node src/aws-device-farm-cli.js list",
    "setup-aws": "node src/aws-device-farm-cli.js setup",
    "start": "node server.js",
    "start:local": "PLAYWRIGHT_BROWSERS_PATH=./playwright-browsers node server.js",
    "dev": "PLAYWRIGHT_BROWSERS_PATH=./playwright-browsers node server.js"
  },
  "keywords": [
    "linkedin",
    "ads",
    "testing",
    "mobile",
    "redirects"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-device-farm": "^3.985.0",
    "aws-sdk": "^2.1693.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "playwright": "^1.48.0",
    "playwright-extra": "^4.3.6",
    "playwright-extra-plugin-stealth": "^0.0.1"
  }
}
